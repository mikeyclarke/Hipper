{% extends 'activity/_activity_feed_activity_layout.twig' %}

{% block text %}
    {% set team_route = path('front_end.app.team.show', {
        subdomain: organization.subdomain,
        team_url_slug: activity.properties.team_url_slug
    }) %}
    <a href="{{ activity.actor.route }}">{{ activity.actor.name }}</a> created the <a href="{{ team_route }}">{{ activity.properties.team_name }}</a> team
{% endblock %}
